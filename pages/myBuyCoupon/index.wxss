/* pages/myBuyCoupon/index.wxss */
.myBuyCoupon_box{
  padding: 0 30rpx 30rpx;
  box-sizing: border-box;
}
.nav_list{
  display: flex;
  justify-content: center;
  align-items: flex-start;
  margin-top: 30rpx;
}
.nav_list view{
  width: 30%;
  text-align: center;
  color: #000;
  font-size: 30rpx;
  border: 1px solid #737c81;
  padding: 10rpx 0;
  box-sizing: border-box;
  border-radius: 20rpx 20rpx 0 0;
}
.nav_list view.on{
  background: rgba(55, 68, 74, 1);
  color: #fff;
}
.table_box{
  border: 1px solid #999999;
  box-sizing: border-box;
  border-bottom: 0;
  margin-top: 30rpx;
}
.table_item{
  display: flex;
  justify-content: space-around;
  align-items: center;
  flex-wrap: nowrap;
  text-align: center;
  padding: 10rpx 0;
  border-bottom: 1px solid #cccccc;
}
.table_th{
  color: #333333;
  font-size: 32rpx;
  width: 50%;
}
.table_th.two{
  width: 30%;
}
.table_td{
  color: #999999;
  font-size: 28rpx;
  width: 50%;
}
.table_td.two{
  width: 30%;
}
.table_td button{
  margin-bottom: 5rpx;
}