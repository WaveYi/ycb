/* pages/huiyibang/moreVideo/index.wxss */
.nav_list{
  display: flex;
  justify-content: center;
  align-items: flex-start;
  margin-top: 30rpx;
}
.nav_list view{
  width: 30%;
  text-align: center;
  color: #000;
  font-size: 30rpx;
  border: 1px solid #737c81;
  padding: 10rpx 0;
  box-sizing: border-box;
  border-radius: 20rpx 20rpx 0 0;
}
.nav_list view.on{
  background: rgba(55, 68, 74, 1);
  color: #fff;
}