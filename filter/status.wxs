      status_txt = '新用户经理';
    }else if(status == '9'){
      status_txt = '老用户经理';
    }else{
      status_txt = '';
    }
    return status_txt;
  },

  logRoleStatus: function(status){
    // 物流角色 0 物流封装站点 1 物流装车站点 2 物流中转站点 3 物流散货站点 
