    }else if(status == 1){
      status_txt = '企业高管';
    }else{
      status_txt = '企业经理'
    }
    return status_txt;
  },

  projectStatus: function(status){
    // 节目类型 0:扫码观看视频 1：节目关注抽奖 2：投票 3：参与者抽奖
