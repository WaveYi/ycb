 var status = {
  filterStatus: function(status){
    var status_txt = '';
    if(status == '0'){
      status_txt = '已邀请';
    }else if(status == '1'){
      status_txt = '已同意';
    }else if(status == '2'){
      status_txt = '已拒绝';
    }
    return status_txt;
  },

  menuStatus: function(status){
    var status_txt = '';
    if(status == '0' || status == null){
      status_txt = '未开始';
    }else if(status == '1'){
      status_txt = '正在进行';
    }else if(status == '2'){
      status_txt = '已结束';
    }else{
      status_txt = '已删除'
    }
    return status_txt;
  },
  
  homeRoleStatus: function(status){
    var status_txt = '';
    if(status == '1'){
      status_txt = '申请中';
    }else if(status == '2'){
      status_txt = '正常';
    }else{
      status_txt = '已拒绝'
    }
    return status_txt;
  },

  roleStatus: function(status){
    // 角色 0 消费者 1 店铺老板 2 治疗经理 3 代理人 4 销售员 5 嘉宾邀请 6 会议观察员 7 物流站点 8 新用户经理 9 老用户经理
    var status_txt = '';
